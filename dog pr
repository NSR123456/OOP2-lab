package com.example.problem2;

public class Dog {
	private String name;
	private String breed;
	public Dog(String name, String breed) {
		this.name = name;
		this.breed = breed;
	}
	public void setter(String name, String breed) {
		this.name = name;
		this.breed = breed;
	}
	public void show() {
		System.out.println(name+ " is a dog of "+ breed+ " breed.");
	}
}
